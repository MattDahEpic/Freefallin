            <kind>1</kind>
            <string>///Set relative x-position, create top portal, and rotate sprite
switch (room) {
    case fall_down:
         x = global.room_end_pos;
         if (global.level != 1) {
            instance_create(x,0,portal_still);
         }
         image_angle = 0;
         break;
    case fall_up:
         x = room_width-global.room_end_pos;
         instance_create(x,room_height,portal_still);
         image_angle = 180;
         break;
    case fall_right:
         y = room_height-global.room_end_pos;
         instance_create(0,y,portal_still);
         image_angle = 90;
         break;
    case fall_left:
         y = global.room_end_pos;
         instance_create(room_width,y,portal_still);
         image_angle = 270;
         break;
}
</string>
